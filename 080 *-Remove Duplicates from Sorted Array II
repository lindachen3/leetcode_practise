public class Solution {
    public int removeDuplicates(int[] nums) {
        int count=1, innercount=1;
        for(int i=1; i<nums.length; i++){
            if(nums[i]==nums[i-1]){
                innercount++;
                if(innercount>2){
                    continue;
                }
            }else{
                innercount=1;
            }
            nums[count]=nums[i];
            count++;
        }
        return count;
    }
}
